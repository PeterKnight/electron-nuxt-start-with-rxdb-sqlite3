{
  "name": "electron-nuxt-starter-with-rxdb-sqlite3",
  "version": "1.0.0",
  "description": "A minimal Electron application, with PouchDB",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-rebuild",
    "dev": "cross-env NODE_ENV=development electron .",
    "build": "nuxt build",
    "pack": "npm run build && electron-packager ./ --overwrite"
  },
  "devDependencies": {
    "babel-preset-vue-app": "^1.2.0",
    "electron": "^1.7.4",
    "electron-devtools-installer": "^2.2.0",
    "electron-rebuild": "^1.4.0",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "cross-env": "^5.0.1",
    "fs": "0.0.1-security",
    "nuxt": "^1.0.0-alpha.4",
    "pouchdb": "^6.1.0",
    "pouchdb-adapter-node-websql": "^6.1.0",
    "rxdb": "^5.0.0",
    "rxjs": "^5.4.1"
  }
}
